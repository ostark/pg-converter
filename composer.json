{
    "name": "ostark/pg-converter",
    "description": "Convert pgsql dump to mysql dump",
    "keywords": ["postgres", "pgres", "mysql", "converter"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Stark",
            "email": "oliver.stark@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "composer/pcre": "^3.1",
        "symfony/console": "^6.0 | ^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.2.0",
        "pestphp/pest": "^2.0.0",
        "phpstan/phpstan": "^1.8.6",
        "symfony/var-dumper": "^6.2.0"
    },
    "autoload": {
        "psr-4": {
            "ostark\\PgConverter\\": "src/"
        },
        "files": ["src/String.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "lint": "pint -v",
        "test:lint": "pint --test -v",
        "test:types": "phpstan analyse --ansi",
        "test:unit": "pest --colors=always",
        "test": [
            "@test:lint",
            "@test:types",
            "@test:unit"
        ]
    }
}
